{
  "extends": ["prettier", "airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "indent": "off",
    "linebreak-style": "off",
    "prettier/prettier": "error",
    "no-underscore-dangle": "off"
  },
  "plugins": ["prettier"]
}
